{
  "name": "prediction-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n wrangler,next -c auto \"npm:dev:wrangler\" \"npm:dev:next\"",
    "dev:next": "next dev --turbopack --port 3000",
    "dev:wrangler": "wrangler pages dev . --port 8788 --local-protocol http",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cf:build": "npm run build",
    "cf:deploy": "npm run build && npx wrangler pages deploy ./out --project-name=prediction-app",
    "cf:seed": "npx tsx scripts/seed-kv.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250926.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.1.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "wrangler": "^4.40.1"
  }
}
