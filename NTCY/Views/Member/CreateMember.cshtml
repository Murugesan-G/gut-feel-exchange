    <script type="text/javascript">
        //$("#MembershipType").change(function () {
        //    //debugger;
        //    var cost = $("#MembershipType option:selected").text();
        //    var len = jQuery.trim(cost);
        //    var array = [];
        //    array = len.split(':');
        //    $('#SubscriptionAmountPaid').val(array[1]);
