        $(document).ready(function () {
            $("#MemberShipAmount").keypress(function (e) {
                if ((e.which != 46 || $(this).val().indexOf('.') != -1) && (e.which < 48 || e.which > 57)) {
                    return false;
                }
            })
            $("#LastSubscriptionPaid").keypress(function (e) {
                if ((e.which != 46 || $(this).val().indexOf('.') != -1) && (e.which < 48 || e.which > 57)) {
                    $('#errmsgLSPaid').css('color', 'red');
                    $("#errmsgLSPaid").html("Invalid Data").show().fadeOut("slow");
                    return false;
                }
            })

            $("#EmailId").keypress(function (e) {
                if (e.which == 32) {
                    return false;
                }
            })

            $("#SubscriptionAmountPaid").keypress(function (e) {
                if ((e.which != 46 || $(this).val().indexOf('.') != -1) && (e.which < 48 || e.which > 57)) {
                    $('#errmsgSAPaid').css('color', 'red');
                    $("#errmsgSAPaid").html("Invalid Data").show().fadeOut("slow");
                    return false;
                }
            })
            $("#memberName").keypress(function (e) {
                if ((e.which >= 33 && e.which <= 64) || (e.which >= 91 && e.which <= 96) ||
                    e.which >= 123 && e.which <= 126) {
                    $('#ememberName').css('color', 'red');
                    $("#ememberName").html("Invalid Data").show().fadeOut("slow");
                    return false;
                }
            })

        });
        //required field validation
        $('#Btn_Submit').click(function () {
            //debugger;

            if ($('#MembershipNo').val() == '' || $('#MembershipNo').val() == "") {
                alert('MembershipNo field is required !!!');
                $('#MembershipNo').focus();
                return false;
            }
            else if ($('#memberName').val() == '' || $('#memberName').val() == "") {
                alert('Please Enter MemberName...')
                $('#memberName').focus();
                return false;
            }
            else if ($('#AdAadharAddressdress').val() == '' || $('#AadharAddress').val() == "") {
                alert('Please Enter Address..')
                $('#AadharAddress').focus();
                return false;
            }
            else if ($('#DOB').val() == '' || $('#DOB').val() == "") {
                alert('Please select Date of Birth..')
                $('#DOB').focus();
                return false;
            }
            else if ($('#Gender').val() == '' || $('#Gender').val() == "") {
                alert('Please Select Gender..')
                $('#Gender').focus();
                return false;
            }
            else if ($('#MobileNo').val() == '' || $('#MobileNo').val() == "") {
                alert('Enter Mobile Number..')
                $('#MobileNo').focus();
                return false;
            }
            else if ($('#EmailId').val() == '' || $('#EmailId').val() == "") {
                alert('EmailId required!!!')
                $('#EmailId').focus();
                return false;
            }
            else if ($('#PrimaryProximatyCard').val() == '' || $('#PrimaryProximatyCard').val() == "") {
                alert('Proximity Card Number is Required!!!')
                $('#PrimaryProximatyCard').focus();
                return false;
            }
            else if ($('#AreasOfInterest').val() == '' || $('#AreasOfInterest').val() == "") {
                alert('Areas Of Interest Field is Required!!!')
                $('#AreasOfInterest').focus();
                return false;
            }
            else if ($('#MemberShipAmount').val() == '' || $('#MemberShipAmount').val() == "") {
                alert('please Enter Initial Membership Amount..')
                $('#MemberShipAmount').focus();
                return false;
            }
            else if ($('#MemberSince').val() == '' || $('#MemberSince').val() == "") {
                alert('MemberSince Field is Required..')
                $('#MemberSince').focus();
                return false;
            }
            else if ($('#DateOfInduction').val() == '' || $('#DateOfInduction').val() == "") {
                alert('Date Of Induction  Field is Required..')
                $('#DateOfInduction').focus();
                return false;
            }
            else if ($('#DateOfExpiry').val() == '' || $('#DateOfExpiry').val() == "") {
                alert('Date Of Expiry  Field is Required..')
                $('#DateOfExpiry').focus();
                return false;
            }
            else { return true; }
        })

        $("#DOB").keypress(function (e) {
            if (e.which <= 47 || e.which >= 58) {
                return false;
            }
        })
        $("#DOBSpouse").keypress(function (e) {
            if (e.which <= 47 || e.which >= 58) {
                return false;
            }
        })
        $("#DOBChild1").keypress(function (e) {
            if (e.which <= 47 || e.which >= 58) {
                return false;
            }
        })
        $("#DOBChild2").keypress(function (e) {
