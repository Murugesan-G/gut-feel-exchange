<script>
    $("#deleteBillModal #btnDeleteBill").click(function () {
        var uname = $('#txtUName').val();
        var pwd = $('#txtPassword').val();
        var mNo = $("#ConTable tbody tr:eq(0) td:eq(0)").html();
        var date = $("#ConTable tbody tr:eq(0) td:eq(2)").html();
        var billType=$("#hdnBillDeleteType").val();
        $.ajax({
            url: "@Url.Content("~/FrontOffice/DeleteBill")",
            type: 'POST',
        cache: false,
        data: JSON.stringify({ 'uname': uname,'pwd':pwd ,"mNo":mNo,"date":date,"billType":billType}),
        contentType: 'application/json; charset=utf-8',
        datatype: 'json',
        success: function (data) {
            $('#alertMsgForDeleteBill').text(data);
        },
        error: function (data) {
            var x = 0;

        },
        });
    })

    function deleteBillAuthentication() {
        $("#deleteBillTitle").text('Please enter Admin/Management credentials');
        $("#alertMsgForDeleteBill").text('');

        $("#unameAuth").css("display", "");
      // $("#txtUName").css("display", "");
       //$("#txtPassword").css("display", "");
       //$("#btnDeleteBill").css("display", "");
       $("#btnEnableDelete").css("display", "none");
       $("#btnDismissDelete").css("display", "none");

    }

    function enableDeleteControls(obj) {
        $("#hdnBillDeleteType").val(obj);
        $("#deleteBillTitle").text('you sure you want to delete this bill?');
        $("#unameAuth").css("display", "none");
       //$("#txtUName").css("display", "none");
       //$("#txtPassword").css("display", "none");
       //$("#btnDeleteBill").css("display", "none");
       $("#btnEnableDelete").css("display", "");
       $("#btnDismissDelete").css("display", "");
   }


    function openCity(evt, cityName) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        document.getElementById(cityName).style.display = "block";
        evt.currentTarget.className += " active";
    }
    // Get the element with id="defaultOpen" and click on it
    document.getElementById("defaultOpen").click();
</script>

